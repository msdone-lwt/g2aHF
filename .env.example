# Gemini API 配置（必须设置）
SECURE_1PSID=your_secure_1psid_here
SECURE_1PSIDTS=your_secure_1psidts_here

# 可选配置
API_KEY=
ENABLE_THINKING=false

# 代理配置（推荐设置）
# VMess 节点模板
# CLASH_PROXIES={
#   "name": "[vmess]节点名称",
#   "type": "vmess",
#   "server": "your_server_ip",
#   "port": your_port,
#   "uuid": "your_uuid",
#   "alterId": 0,
#   "cipher": "auto",
#   "udp": true,
#   "tls": true,
#   "skip-cert-verify": true,
#   "servername": "your_servername",
#   "network": "ws",
#   "ws-opts": {
#     "path": "/your_path",
#     "headers": {"Host": "your_host"}
#   }
# }

# 其他代理类型示例：
# Trojan 节点
# CLASH_PROXIES={"name": "[trojan]美国", "type": "trojan", "server": "trojan.example.com", "port": 443, "password": "your_password", "tls": true}

# Shadowsocks 节点
# CLASH_PROXIES={"name": "[ss]新加坡", "type": "ss", "server": "ss.example.com", "port": 8388, "cipher": "aes-256-gcm", "password": "your_password"}

# HTTP 代理
# CLASH_PROXIES={"name": "HTTP代理", "type": "http", "server": "proxy.example.com", "port": 8080, "username": "user", "password": "pass"}

# SOCKS5 代理
# CLASH_PROXIES={"name": "SOCKS5代理", "type": "socks5", "server": "proxy.example.com", "port": 1080, "username": "user", "password": "pass"}